<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bottombar.sample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="25" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.0" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.bottombar.sample.MainActivity"
            android:launchMode="singleInstance" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.bottombar.sample.ThreeTabsActivity" />
        <activity android:name="com.example.bottombar.sample.IconsOnlyActivity" />
        <activity
            android:name="com.example.bottombar.sample.FiveColorChangingTabsActivity"
            android:theme="@style/AppTheme.TransNav" />
        <activity
            android:name="com.example.bottombar.sample.ThreeTabsQRActivity"
            android:theme="@style/AppTheme.TransNav" />
        <activity android:name="com.example.bottombar.sample.BadgeActivity" />
        <activity android:name="com.example.bottombar.sample.CustomColorAndFontActivity" />
    </application>

</manifest>